<html>
<head> 
	<title>Video streaming with Node.js</title> 
</head> 
<body> 
<%if(!currentUser){%>
	<a href="/">Login</a>
	<%} else{%>
		<a href="/profile"><p><%=currentUser.name%></p></a>
		<%courses.forEach(function(course){%>
            <div><a href="/courses/<%=course._id%>"><%=course.title%></a></div>
            <%})%>
		<br>
		<a href="/logout">Logout</a>
	<%}%>
</body> 
</html>